   <string notr="true"> /* Global Styling */
QMainWindow {
	background-color: #18181B;
	font: 9pt &quot;Aux Mono&quot;;
}

QWidget{
	background-color: #18181B;
	color: #bebebe;
	border: none;
	font:900  9pt &quot;Aux Mono&quot;;
}

QToolTip {
	border-radius: 5px;
	background-color:#252528;
    padding: 4px 6px; 
	font: 8pt &quot;Aux Mono&quot;;
}
QLineEdit{
	color: #908f8f;
}
QLineEdit:focus{
	color: #bebebe;
}

QMenu {
    background-color: #18181B;
    border: 1px solid #18181B;
    color: #bebebe;
    font:  10pt &quot;Aux Mono&quot;;
	border-radius: 5px;
	padding: 5px;
}
QMenu::item {
    background-color: transparent;
    color: #bebebe;
	border-radius: 5px;
	padding: 2px 70px 2px 70px;
}
QMenu::item:selected {
    background-color: #3b3b3e;
}

#terminalInput,
QTextBrowser{
	font: 9pt &quot;Aux Mono&quot;;
}

/* buttons */

QPushButton{
	background-color:#252528;
	border-style: outset;
	qproperty-iconSize: 16px;
	border-radius: 5px;
}

#settingsPageSideBarContainer QPushButton{
	background-color:#18181B;
	border-radius: 5px;
}

#settingsPageSideBarContainer QPushButton:hover,
QPushButton:hover{
	background-color:#3b3b3e;
}

#settingsPageSideBarContainer QPushButton:pressed,
QPushButton:pressed{
	background-color:#1e1e20;
}
#connectionManagementFrame QPushButton:checked,
#topButtons QPushButton:checked{
	background-color:#7662D0;
	border: 2px solid #997662D0;
}
#dasboardMainButtons QPushButton:checked{
	border:none;
	border-right:2px solid #7662D0;
	border-bottom:2px solid #7662D0;
}

/* COMBO BOX */

QComboBox{
	background-color:#252528;
	border-style: outset;
	qproperty-iconSize: 16px;
	border-radius: 5px;
}
QComboBox:hover {
	background-color:#515153;
}
QComboBox QListView {
    background-color: #212124;
	outline: none;
	border-radius: 5px;
	border:none;
}
QComboBox QListView:item {
    min-height: 27px;
    padding: 2px 10px;
	margin:3px;
	border-radius: 5px;
}
QComboBox QListView:item:hover {
    background-color:#515153;
	outline: none;
	border:none;
}
QComboBox::down-arrow {
	image: url(:/icons/icons/chevron-down-dark.svg);
	width: 14px;
	height: 14px;
	padding-right: 10px;
}
QComboBox:drop-down {
    border: 0px;
    border-radius: 0px;
}

/* labels */

#settingsInfoLabel {
	color: #383838;
	font: 7pt &quot;Aux Mono&quot;;
}
#settingsScrollContainer QLabel{
	font: 10pt &quot;Aux Mono&quot;;
	color: #6e6e6e;
}
#stateLabel {
	border-radius: 5px;
	background-color:#252528;
}
#scrollTlmContainer QFrame,
#tlmFramesContainer QFrame {
	border-radius: 5px;
	background-color:#252528;
}

/* Status Bar */

#customStatusBarFrame QLabel{
	color: #383838;
	font: 900 7pt &quot;Aux Mono&quot;;
	margin-bottom: 2px;
}
#line, #line_2, #line_3, #line_4, #line_5,
#line_6, #line_7, #line_8, #line_9, #line_10{
    border: 0px;
    background-color: #383838;
}

/* Scrollbar */

QScrollBar {
    border: 0;
    width: 10px;
    background-color: #18181B; 
}
QScrollBar::handle:vertical {
    background-color: #252528;
    min-height: 30px;
    max-height: 30px;
    border-radius: 5px; 
    margin-top: 2px;
    margin-bottom: 2px;
}
QScrollBar::handle::vertical:hover {
    background-color: #3b3b3e; 
}

QScrollBar::sub-line, QScrollBar::add-line:vertical {
    border: none;
    height: 0; 
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical,
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
	background: none;
}
/* SETTING FORM */

#SettingsForm QFrame{
	border-radius:5px;
	background-color:#252528;
}
#SettingsForm #container QFrame{
	border:none;
}
/* information */

#SettingsForm #profile QLabel{
	background-color:#3b3b3e;
	border: 2px solid #212124;
}
#SettingsForm #name{
	color: #bebebe;
	font: 900 12pt &quot;Aux Mono&quot;;
}
#SettingsForm #description{
	color: #6e6e6e;
	font: 400 10pt &quot;Aux Mono&quot;;
}
#SettingsForm #restart{
	color: #a53d3d;
	font: 800 9pt &quot;Aux Mono&quot;;
}

/*controls*/

#SettingsForm QPushButton{
	font: 700 10pt &quot;Aux Mono&quot;;
