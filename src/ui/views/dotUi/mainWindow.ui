	padding: 0px 4px 0px 4px;
}
#SettingsForm QPushButton:hover {
	background-color:#515153;
}
#SettingsForm QPushButton:pressed {
	background-color:#1e1e20;
}
#SettingsForm #button{
	border-radius: 5px;
}

