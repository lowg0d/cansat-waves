}
#SettingsForm QComboBox:drop-down {
    border: 0px;
    border-radius: 0px;
}

#SettingsForm QLineEdit{
	font: 700 10pt &quot;Aux Mono&quot;;
	color: #bcbcbc;
	background-color:#3b3b3e;
	border-radius: 5px;
	padding-left: 5px;
	padding-right: 5px;
}
#SettingsForm QLineEdit:focus {
	color: #bebebe;
}
#SettingsForm QLineEdit:hover {
	background-color:#515153;
}
#SettingsForm #numbers_2 QDoubleSpinBox,
#SettingsForm #numbers QSpinBox{
	font: 700 10pt &quot;Aux Mono&quot;;
	color: #bebebe;
	background-color:#3b3b3e;
	border-left: 0px;
	border-right:0px;
	text-align: center;
}
#SettingsForm #numbers_2 #plus_2,
#SettingsForm #numbers #plus{
	border-left: 0px;
	border-top-right-radius: 5px;
	border-bottom-right-radius: 5px;
}
#SettingsForm #numbers_2 #minus_2,
#SettingsForm #numbers #minus{
	border-right: 0px;
	border-top-left-radius: 5px;
	border-bottom-left-radius: 5px;
}

/* toggle */

#SettingsForm #off {
	border-right: 0px;
	border-top-left-radius: 5px;
	border-bottom-left-radius: 5px;
}
#SettingsForm #on {
	border-left: 0px;
	border-top-right-radius: 5px;
	border-bottom-right-radius: 5px;
}
#SettingsForm #off:checked {
	font: 900 10pt &quot;Aux Mono&quot;;
	background-color: #4c272a;
	border: 1px solid #261315;
}
#SettingsForm #on:checked {
	font: 900 10pt &quot;Aux Mono&quot;;
	background-color: #627254;
	border: 1px solid #21261c;
}

